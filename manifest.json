{
  "manifest_version": 2,
  "version": "1.0",
  "name": "Copy Forecast project and task id",
  "description": "Simple extension that copies the current forecast project and task id",
  "icons": {
    "16": "images/ghForecast.png",
    "48": "images/ghForecast.png",
    "128": "images/ghForecast.png"
  },
  "browser_action": {
    "default_icon": "images/ghForecast.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ]
}

